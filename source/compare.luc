module compare (
    input z,
    input v,
    input n,
    input alufn[6],
    output cmp
) {
    always {
        cmp = 0
        case (alufn[2:1]) {
            0:
                cmp = 0
            1:
                cmp = z
            2: 
                cmp = n^v
            3:
                cmp = (n^v) | z
        }
    }
}